{
  "name": "nextpocket",
  "version": "0.1.0",
  "engines": {
    "node": ">=18",
    "yarn": ">=3"
  },
  "private": true,
  "author": "Guy Romelle Magayano <https://guyromellemagayano.dev>",
  "license": "MIT",
  "scripts": {
    "build": "yarn i18n && next build",
    "commit": "git cz",
    "component:headless": "cypress run --component",
    "component": "cypress open --component",
    "i18n": "i18nexus pull",
    "dev": "yarn i18n && next dev",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "graphql-codegen:generate": "graphql-codegen -r dotenv/config --config codegen.ts",
    "graphql-codegen:watch": "graphql-codegen --watch -r dotenv/config --config codegen.ts",
    "lint:fix": "eslint --quiet --fix --color --ignore-path .gitignore",
    "lint:format": "prettier --write --ignore-path .gitignore",
    "lint": "yarn i18n && yarn lint:fix && yarn lint:format",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "setup": "./bin/setup.sh",
    "start": "yarn i18n && next start",
    "test:ci": "jest --ci",
    "test": "jest --watchAll"
  },
  "dependencies": {
    "@contentful/live-preview": "^2.11.9",
    "@contentful/rich-text-react-renderer": "^15.19.0",
    "@contentful/rich-text-types": "^16.3.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.2",
    "@next/bundle-analyzer": "^14.0.0",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.0.5",
    "accept-language": "^3.0.18",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "dotenv": "^16.3.1",
    "focus-visible": "^5.2.0",
    "graphql": "^16.8.1",
    "i18next": "^23.6.0",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.3.0",
    "i18next-resources-to-backend": "^1.1.4",
    "intersection-observer": "^0.12.2",
    "lodash": "^4.17.21",
    "next": "^14.0.0",
    "next-auth": "^4.24.3",
    "next-compose-plugins": "^2.2.1",
    "next-i18n-router": "^5.0.1",
    "next-i18next": "^14.0.3",
    "pocketbase": "^0.19.0",
    "postcss-focus-visible": "^9.0.0",
    "postgres": "^3.4.1",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-i18next": "^13.3.1",
    "react-kawaii": "^0.18.0",
    "react-transition-group": "^4.4.5",
    "rehype-parse": "^9.0.0",
    "rehype-react": "^8.0.0",
    "remark-breaks": "^4.0.0",
    "server-only": "^0.0.1",
    "sharp": "^0.32.6",
    "styled-jsx": "^5.1.2",
    "swr": "^2.2.4",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@graphql-codegen/typescript-react-query": "^6.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-query-devtools": "^5.4.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/circular-dependency-plugin": "^5.0.7",
    "@types/imagesloaded": "^4.1.4",
    "@types/jest": "^29.5.6",
    "@types/lodash": "^4.14.200",
    "@types/next-pwa": "^5.6.8",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/react-transition-group": "^4.4.8",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "autoprefixer": "10.4.16",
    "circular-dependency-plugin": "^5.2.2",
    "commitizen": "^4.3.0",
    "contentful-import": "^9.4.5",
    "cypress": "^13.3.3",
    "eslint": "^8.52.0",
    "eslint-config-next": "14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-testing-library": "^6.1.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "i18nexus-cli": "^3.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-axios": "^4.7.3",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "start-server-and-test": "^2.0.1",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty",
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true",
      "commit": "git cz",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json}": [
      "yarn lint"
    ]
  },
  "packageManager": "yarn@3.6.3"
}
